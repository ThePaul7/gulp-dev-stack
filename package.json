{
  "name": "gulp-dev-stack",
  "version": "1.0.0",
  "description": "Gulp dev-stack",
  "engines": {
    "node" : ">=5"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  },
  "eslintConfig": {
    "plugins": [
      "babel",
      "import"
    ],
    "extends": [
      "airbnb-base",
      "airbnb-base/legacy"
    ],
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jquery": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "rules": {
      "no-console": 2
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "indentation": 4,
      "selector-pseudo-element-colon-notation": "single",
      "declaration-empty-line-before": "never",
      "number-leading-zero": null
    }
  },
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-core": "^6.5.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "bootstrap": "^4.0.0-alpha.2",
    "browser-sync": "^2.15.0",
    "browserify": "^13.1.0",
    "copy-paste": "^1.1.3",
    "cssnano": "^3.7.4",
    "del": "^2.2.2",
    "envify": "^3.4.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb-base": "^7.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.14.0",
    "glob": "^7.0.6",
    "gulp": "^3.9.0",
    "gulp-css-globbing": "^0.1.8",
    "gulp-eslint": "^3.0.1",
    "gulp-if": "^2.0.1",
    "gulp-nunjucks": "^2.1.1",
    "gulp-plumber": "^1.0.1",
    "gulp-postcss": "^6.0.1",
    "gulp-prettify": "^0.5.0",
    "gulp-real-favicon": "^0.2.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-stylelint": "^3.2.0",
    "gulp-svgmin": "^1.2.2",
    "gulp-svgstore": "^6.0.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.6",
    "postcss-flexbugs-fixes": "^2.0.0",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.4",
    "stylelint": "^7.2.0",
    "stylelint-config-standard": "^13.0.0",
    "uglifyify": "^3.0.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.4.0",
    "yargs": "^5.0.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "bootstrap-sass": "^3.3.6",
    "svgxuse": "^1.1.21"
  }
}
